#include<iostream>
using namespace std;
int binarysearch(int arr[], int size, int key){
    int start =0;
    int end= size-1;
    int mid= start+(end-start)/2;
    while(start<=end){
        if(arr[mid]==key){
            return mid;

        }
        else if(arr[mid]<key){
            start=mid+1;
        }
        else{
            end=mid-1;
        }
        mid=start+(end-start)/2;


    }
    return -1;

}
int main(){
    int a[]={1,2,3,4,5,6,7,8,9};
    int b=sizeof(a);
    cout<<"Enter the value of key to be found"<<endl;
    int key;
    cin>>key;
    int result= binarysearch(a,b,key);
    cout<<"The index of given element is:"<<result;
    

}
